int length(Node *head) {
    /* Don't write main().
     * Don't read input, it is passed as function argument.
     * Return output and don't print it.
     * Taking input is handled automatically.
     */
    if(head->next==NULL){
        return 1;
    }
    else{
        head=head->next;
        int small_len=length(head);
        int total_length=small_len+1;
    return total_length;
    }
    }
